{{#render-area 'application' as |api|}}
  {{#api.area filteredModels=filteredModels}}
    {{outlet}}
  {{/api.area}}
{{else}}
  <div class="ember-admin">
    <h2>Admin</h2>
    {{#each filteredModels as |name|}}
      {{link-to name 'model-records' name}}
    {{/each}}
    {{outlet}}
  </div>
{{/render-area}}
